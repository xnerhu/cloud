{
  "name": "wexond-cloud",
  "private": true,
  "scripts": {
    "dev:updates": "ibazel run //services/updates:updates_dev",
    "seed:updates": "bazel run //services/updates:schema_up && bazel run //services/updates:seed",
    "drop:updates": "bazel run //services/updates:schema_drop",
    "build:updates": "lerna exec yarn build --scope=@backend/{common,updates}",
    "build:website": "lerna exec yarn build --scope=@web/website",
    "analyze": "lerna run --stream --parallel analyze",
    "clean": "bazel clean",
    "deps": "npx lerna link",
    "ci-build": "ts-node -T scripts/ci-build.ts",
    "test-old": "lerna run --stream --parallel test",
    "build": "bazel build //...",
    "test": "bazel test //... --build_tests_only",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "prisma:updates": "npx prisma generate --schema ./services/updates/prisma/schema.prisma",
    "prisma": "concurrently \"npm run prisma:updates\"",
    "lint": "eslint \"common/**/src/**\" \"services/**/src/**\" --ext \".js,.jsx,.ts,.tsx\"",
    "lint-cache": "npx run lint -- --cache --cache-strategy content",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
