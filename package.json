{
  "name": "wexond-cloud",
  "private": true,
  "scripts": {
    "dev:reports": "lerna exec yarn dev --parallel --scope=@services/{common,reports}",
    "watch:reports": "lerna exec yarn watch --scope=@services/reports",
    "analyze": "lerna run --stream --parallel analyze",
